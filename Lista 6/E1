#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int N;
    cin >> N;
    while (N--) {
        int X;
        cin >> X;
        unsigned long long graos = (1ULL << X) - 1; 
        unsigned long long gramas = graos / 12;
        unsigned long long kilogramas = gramas / 1000;
        cout << kilogramas << " kg" << endl;
    }
    return 0;
}
